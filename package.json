{
  "name": "shoppinglist",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm-run-all -s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "dev": "npm-run-all -p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "dev:node": "cross-env NODE_ENV=development node server/index.js --watch server/index.js",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch"
  },
  "prettier": {
    "useTabs": true,
    "tabWidth": 4
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "@remix-run/express": "^1.6.7",
    "@remix-run/node": "^1.6.7",
    "@remix-run/react": "^1.6.7",
    "@remix-run/serve": "^1.6.7",
    "@types/styled-components": "^5.1.25",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "isbot": "^3.5.0",
    "morgan": "^1.10.0",
    "node-persist": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "styled-components": "^5.3.5",
    "tailwindcss": "^3.1.6",
    "tiny-invariant": "^1.2.0",
    "web-push": "^3.4.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@remix-run/dev": "^1.6.7",
    "@remix-run/eslint-config": "^1.6.7",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.4.5",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.4",
    "@types/node-persist": "^3.1.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.7",
    "binode": "^1.0.5",
    "c8": "^7.11.3",
    "cookie": "^0.5.0",
    "cypress": "^10.3.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^6.0.3",
    "msw": "^0.44.1",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "prisma": "^4.0.0",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.18.0"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "node --require tsconfig-paths/register prisma/seed.js"
  }
}
